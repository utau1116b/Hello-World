@RENAME()
PRINTFORML どれをリネームしますか？
PRINTL 
PRINTL [1] 調教コマンド
PRINTL [2] アイテム
PRINTL [3] 素質
PRINTL [4] 能力
PRINTL [5] 基礎
PRINTL [6] 経験
PRINTL [7] 敵名
PRINTL [] 
PRINTL
PRINTL [0] 戻る
$INPUT_LOOP
INPUT
IF RESULT == 1
	CALL RENAME_TRAIN
ELSEIF RESULT == 2
	;CALL RENAME_ITEM
ELSEIF RESULT >= 3 && RESULT <= 6
	CALL RENAMES(RESULT)
ELSEIF RESULT == 7
	CALL RENAME_ENEMY
ELSE
	RETURN
ENDIF
@RENAME_TRAIN
REPEAT 10
	PRINTL 
REND
REPEAT 1000
	IF STRLENS(TRAINNAME:COUNT) > 0
		PRINTFORML {COUNT} , 調教:%TRAINNAME:COUNT%
	ELSE
		CONTINUE
	ENDIF
REND
@RENAMES(ARG)
REPEAT 10
	PRINTL 
REND
IF ARG == 3
	LOCALS = TALENT
	LOCALS:2 = "素質"
	REPEAT 1000
		IF STRLENS(TALENTNAME:COUNT) > 0
			PRINTFORML TALENT:{COUNT} , 素質:%TALENTNAME:COUNT%
		ELSE
			CONTINUE
		ENDIF
	REND
ELSEIF ARG == 4
	LOCALS = ABL
	LOCALS:2 = "能力"
	REPEAT 1000
		IF STRLENS(ABLNAME:COUNT) > 0
			PRINTFORML ABL:{COUNT} , 能力:%ABLNAME:COUNT%
		ELSE
			CONTINUE
		ENDIF
	REND
ELSEIF ARG == 5
	LOCALS = BASE
	LOCALS:2 = "基礎"
ELSEIF ARG == 6
	LOCALS = EXP
	LOCALS:2 = "経験"
	REPEAT 1000
		IF STRLENS(EXPNAME:COUNT) > 0
			PRINTFORML EXP:{COUNT} , 経験:%EXPNAME:COUNT%
		ELSE
			CONTINUE
		ENDIF
	REND
ENDIF
LOCALS:1 = LOCALS + "NAME"

;1に代入するので、セーブしてはいけない
@RENAME_ENEMY
FOR LOCAL, 0, 2200
	TRYCCALLFORM SET_ENEMY_STATUS_{LOCAL}, 1, 1, 1
		PRINTFORML {LOCAL} , キャラ:%CALLNAME:1%
	CATCH
		CONTINUE
	ENDCATCH
NEXT