		SOURCE:5 = 200
		TIMES SOURCE:8 , 1.50
	ELSEIF ABL:6 == 3
		SOURCE:4 = 350
		SOURCE:5 = 300
		TIMES SOURCE:8 , 1.00
	ELSEIF ABL:6 == 4
		SOURCE:4 = 450
