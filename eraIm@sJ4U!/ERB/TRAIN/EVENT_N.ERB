LOCAL:1 = 20
LOCAL:2 = 21
LOCAL:3 = 22
LOCAL:4 = 27
LOCAL:5 = 31
LOCAL:6 = 32
LOCAL:7 = 34
LOCAL:8 = 37
LOCAL:9 = 63
REPEAT 10
	IF TALENT:T:(LOCAL:COUNT)
		;初めだけ名前表示
		IF LOCAL:99 == 0
			PRINTS NAME:T
