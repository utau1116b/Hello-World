@GET_CHILD_M_TO_T
;マスターの精液でターゲット妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数
IF TARGET != -1
	IF TALENT:TARGET:134 == 0 && CFLAG:TARGET:104 == 1
		PRINTFORMW %CALLNAME:TARGET%の様子がおかしい…
		PRINTFORML %CALLNAME:TARGET%は子供を身篭った様だ
		PRINTFORMW %NAME:TARGET%は[%TALENTNAME:134%]した
		TALENT:TARGET:134 = 1

		T = TARGET
		CALL CHANGE_N_STATUS

		;妊娠時の口上をイベント口上でやってみるのはいいとして分岐どうしよう…
		;基本的には口上側で分岐してもらう方向で
		TFLAG:13 = 20
		CALL SELF_KOJO
	ENDIF
ENDIF

@GET_CHILD_M_TO_A
;マスターの精液で助手妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数

;助手がいるか
IF ASSI >= 1
	IF TALENT:ASSI:134 == 0 && CFLAG:ASSI:104 == 1
		PRINTFORMW %CALLNAME:ASSI%の様子がおかしい…
		PRINTFORML %CALLNAME:ASSI%は子供を身篭った様だ
		PRINTFORMW %NAME:ASSI%は[%TALENTNAME:134%]した
		TALENT:ASSI:134 = 1

		T = ASSI
		CALL CHANGE_N_STATUS

		;主人が助手を妊娠させた時のパートナーの口上
		TFLAG:13 = 20
		CALL SELF_KOJO
		;口上用にターゲットを待避してから助手の番号を入れる
		Z = TARGET
		TARGET = ASSI
		TFLAG:13 = 94
		CALL SELF_KOJO
		TARGET = Z
	ENDIF
ENDIF

@GET_CHILD_A_TO_T
;助手の精液で奴隷妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数

