	;達成感
	SOURCE:5 += LOCAL /12
ELSEIF TEQUIP:65 == 17
	;PRINTL ＜コスプレ中[裸リボン]＞
	;露出度超
	;愛のためならこのくらい…
	TIMES LOCAL , 2.50
	TIMES LOCAL:2 , 2.00
	IF TIME:4 & 2
		;結婚記念日
		IF TIME:1 == 17 || TIME:1 == 44 || TIME:1 == 47|| TIME:1 == 48
			;イベント時愛情経験+5
			EXP:23 += 5
		ELSE
