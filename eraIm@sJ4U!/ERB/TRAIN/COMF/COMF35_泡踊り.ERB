;-------------------------------------------------
;泡踊り
;奉仕系コマンド、レベル3
;-------------------------------------------------
@COM35
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 8
;強度
TCVAR:PLAYER:4 = 2000

LOCALS = {PLAYER}/
CSTR:TARGET:10 += LOCALS
TCVAR:TARGET:2 = 8
;強度
TCVAR:TARGET:4 = 2000
;奉仕快楽
TFLAG:奉仕快楽 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 50
DOWNBASE:気力 += 180
;DOWNBASE:疲労 += 80

SOURCE:快Ｃ = 200
SOURCE:快Ｂ = 200
SOURCE:情愛 = 150
SOURCE:性行動 = 450
SOURCE:達成 = 250
SOURCE:屈従 = 1200
SOURCE:逸脱 = 600
SOURCE:反感 = 600

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:反感 += 1000
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の汚れ
STAIN:口 = 0
STAIN:手 = 0
STAIN:Ｐ = 2
STAIN:Ｖ = 1
STAIN:Ａ = 8
STAIN:Ｂ = 0
STAIN:膣内 = 0

;調教者の汚れ
IF ASSIPLAY == 0
	STAIN:MASTER:口 = 0
	STAIN:MASTER:手 = 0
	STAIN:MASTER:Ｐ = 2
	STAIN:MASTER:Ｖ = 1
	STAIN:MASTER:Ａ = 8
	STAIN:MASTER:Ｂ = 0
	STAIN:MASTER:膣内 = 0
ELSE
	STAIN:ASSI:口 = 0
	STAIN:ASSI:手 = 0
	STAIN:ASSI:Ｐ = 2
	STAIN:ASSI:Ｖ = 1
	STAIN:ASSI:Ａ = 8
	STAIN:ASSI:膣内 = 0
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(39 + HETEROSEX(TARGET,PLAYER)) += 7
	EXP:PLAYER:(39 + HETEROSEX(TARGET,PLAYER)) += 7
ENDIF

;愛情経験
SIF TALENT:恋慕 && ASSIPLAY == 0
	EXP:愛情経験 += 1
RETURN 1

@CAN_COM35(ARG)
SKIPDISP ARG
PRINTL 泡踊り
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
	S = 1
ENDIF

IF MARK:快楽依存
	SIF S
		PRINT  + 
	A += MARK:快楽依存 * 1
	PRINT 快楽依存
	PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 1,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	S = 1
ENDIF
;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 3
	PRINT 恥じらい
	PRINTV '(,3,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
		PRINT  + 
	A += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	PRINT  - 
	A -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
 	PRINT 献身的
 	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && !TALENT:淫乱
	PRINT  - 
	A -= 3
	PRINT 快感の否定
	PRINTV '(,3,')
	S = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:男嫌い && !(TALENT:PLAYER:オトコ) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	A -= 12
	PRINT 男嫌い
	PRINTV '(,12,')
	S = 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 恋慕
	PRINTV '(,5,')
	S = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 親愛
	PRINTV '(,5,')
	S = 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SIF S
		PRINT  + 
	A += 8
	PRINT ふたなり
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:Ｐ & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:Ｐ & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:Ｐ & 8
	Y += 7
SIF STAIN:PLAYER:Ｐ & 16
	Y += 15

Y = Y * (2 - TALENT_LV(TARGET, "汚臭耐性")) / 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭敏感
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(30以上で実行)
PRINT  = 
PRINTV A

V = 30
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
RETURN 1

