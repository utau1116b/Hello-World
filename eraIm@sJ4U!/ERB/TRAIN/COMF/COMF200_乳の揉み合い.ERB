;-------------------------------------------------
;乳の揉み合い
;助手によるコマンド、及びレズプレイ、レベル1
;-------------------------------------------------
@COM200
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 7
DOWNBASE:気力 += 70
;DOWNBASE:疲労 += 50

SOURCE:快Ｂ = 200
SOURCE:性行動 = 150
SOURCE:露出 = 50
SOURCE:不潔 = 30
SOURCE:逸脱 = 100
SOURCE:反感 = 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:反感 += 100

;調教者が［幼児］
IF TALENT:PLAYER:幼児
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行動 , 1.20
ENDIF

;調教者が［幼稚］
IF TALENT:PLAYER:幼稚
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行動 , 1.20
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の指の汚れが移動
	STAIN:Ｂ |= STAIN:PLAYER:手
	STAIN:PLAYER:手 |= STAIN:Ｂ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(39 + HETEROSEX(TARGET,PLAYER)) += 7
	EXP:PLAYER:(39 + HETEROSEX(TARGET,PLAYER)) += 7
ENDIF

RETURN 1

