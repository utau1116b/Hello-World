@COM46
;浣腸器＋プラグ
IF TEQUIP:90
	PRINTL 触手浣腸
	STR:0 = 触手浣腸
ELSE
	PRINTL 浣腸器＋プラグ
	STR:0 = 浣腸器＋プラグ
ENDIF

DOWNBASE:0 += 60
DOWNBASE:1 += 150

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	SOURCE:2 = 80
	SOURCE:13 = 300
ELSEIF ABL:5 == 1
	SOURCE:2 = 250
	SOURCE:13 = 800
ELSEIF ABL:5 == 2
	SOURCE:2 = 600
	SOURCE:13 = 1400
ELSEIF ABL:5 == 3
	SOURCE:2 = 1000
	SOURCE:13 = 1800
ELSEIF ABL:5 == 4
	SOURCE:2 = 1300
	SOURCE:13 = 2100
ELSE
	SOURCE:2 = 700 + (ABL:5 * 200)
	SOURCE:13 = 1650 + (ABL:5 * 150)
ENDIF

;ABL8,マゾっ気をみる
IF ABL:8 < 1
	SOURCE:6 = 2000
	SOURCE:8 = 1000
	SOURCE:13 += 200
	SOURCE:14 = 1000
	SOURCE:15 = 2000
ELSEIF ABL:8 < 2
	SOURCE:6 = 1600
	SOURCE:8 = 2000
	SOURCE:13 += 500
	SOURCE:14 = 1000
	SOURCE:15 = 1000
ELSEIF ABL:8 < 3
	SOURCE:6 = 1200
	SOURCE:8 = 1000
	SOURCE:13 += 800
	SOURCE:14 = 1000
	SOURCE:15 = 500
ELSEIF ABL:8 < 4
	SOURCE:6 = 800
	SOURCE:8 = 1000
	SOURCE:13 += 1200
	SOURCE:14 = 1000
	SOURCE:15 = 100
ELSEIF ABL:8 < 5
	SOURCE:6 = 600
	SOURCE:8 = 1000
	SOURCE:13 += 1500
	SOURCE:14 = 1000
	SOURCE:15 = 0
ELSE
	SOURCE:6 = 400
	SOURCE:8 = 1000
	SOURCE:13 += 750 + (ABL:8 * 250)
	SOURCE:14 = 1000
	SOURCE:15 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:0 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:0 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:0 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:0 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	SOURCE:2 = SOURCE:2 * (ABL:0 * 5 + 105) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:15 == 0
	TIMES SOURCE:6 , 1.00
ELSEIF ABL:PLAYER:15 == 1
	TIMES SOURCE:6 , 1.05
ELSEIF ABL:PLAYER:15 == 2
	TIMES SOURCE:6 , 1.10
ELSEIF ABL:PLAYER:15 == 3
	TIMES SOURCE:6 , 1.20
ELSEIF ABL:PLAYER:15 == 4
	TIMES SOURCE:6 , 1.30
ELSE
	SOURCE:6 = SOURCE:6 * (ABL:PLAYER:15 * 5 + 125) / 100
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 5
PRINTL Ａ経験＋5

;CALL TRAIN_MESSAGE_B

;浣腸プラグの着脱
IF TEQUIP:90
	IF TEQUIP:46
		TEQUIP:46 = 0
		IF CFLAG:4 == 0
			EXP:50 += 1
			PRINTL 異常経験＋１
			CFLAG:4 = 1
		ENDIF
	ELSE
		TEQUIP:46 = 1
		STAIN:Ａ |= 2
		STAIN:Ａ |= 4
	ENDIF
	T = 0
ELSE
	IF TEQUIP:46
		TEQUIP:46 = 0
		IF CFLAG:4 == 0
			EXP:50 += 1
			PRINTL 異常経験＋１
			CFLAG:4 = 1
		ENDIF
	ELSE
		TEQUIP:46 = 1
	ENDIF
ENDIF

RETURN 1


