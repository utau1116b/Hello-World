;-------------------------------------------------
;シックスナイン
;愛撫・奉仕系派生コマンド
;-------------------------------------------------
@SCOM1
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 4
;強度
TCVAR:PLAYER:4 = 1500

LOCALS = {PLAYER}/
CSTR:TARGET:10 += LOCALS
TCVAR:TARGET:2 = 4
;強度
TCVAR:TARGET:4 = 1500
;奉仕快楽
TFLAG:30 = 1

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 20
DOWNBASE:気力 += 160

SOURCE:快Ｃ = 100
SOURCE:情愛 = 100
SOURCE:性行動 = 450
SOURCE:達成 = 200
SOURCE:液体 = 80
SOURCE:露出 = 100
SOURCE:屈従 = 800
SOURCE:逸脱 = 600
SOURCE:反感 = 600

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*80 + 50
IF SELECTCOM == [[調教:指挿入れ]]
	;破瓜抑制
	TFLAG:破瓜抑制 = 1
	IF !TALENT:処女
		SOURCE:快Ｖ += 120
		SOURCE:痛み += 15
		SOURCE:露出 += 10
		SOURCE:逸脱 += 20
		SOURCE:反感 += 20
	;V経験が0の時はV経験を伴わない物に派生
	;Vの珠は入らないが、快Vが高いと慣らしてあるため処女喪失時に影響
	ELSE
		SOURCE:快Ｖ += 60
		SOURCE:痛み += 10
		SOURCE:露出 += 30
		SOURCE:逸脱 += 30
		SOURCE:反感 += 30
	ENDIF
	;Vへの苦痛
	CALL PAIN_CHECK_V(TARGET)
	;-------------------------------------------------
	;汚れの処理
	;-------------------------------------------------
	;奴隷の膣内⇔調教者の指の汚れが移動
		STAIN:膣内 |= STAIN:PLAYER:手
		STAIN:PLAYER:手 |= STAIN:膣内
ELSEIF SELECTCOM == [[調教:アナル愛撫]]
	SOURCE:快Ａ += 100
	SOURCE:痛み += 100
	SOURCE:屈従 += 100
	SOURCE:逸脱 += 80
	SOURCE:反感 += 60

	;Aへの苦痛は先に処理しておく
	CALL PAIN_CHECK_A(TARGET)
	;-------------------------------------------------
	;汚れの処理
	;-------------------------------------------------
	;奴隷のＡ⇔調教者の指の汚れが移動
	STAIN:PLAYER:手 |= STAIN:Ａ
	STAIN:PLAYER:手 |= STAIN:腸内
	STAIN:Ａ |= STAIN:PLAYER:手
	STAIN:腸内 |= STAIN:PLAYER:手
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:反感 += 800

;舌を使う調教では、調教者の舌使いによって、快感のソース増加
SIF TALENT:PLAYER:舌使い
	TIMES SOURCE:快Ｃ , 1.50

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF GETBIT(FLAG:62 ,22)
	CALL SOURCE_M_COM69

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者のＶの汚れが移動（半人半妖、ふたなり、オトコはペニス）
IF CHECK_PENIS(PLAYER)
	STAIN:口 |= STAIN:PLAYER:Ｐ
	STAIN:PLAYER:Ｐ |= STAIN:口
ELSE
	STAIN:口 |= STAIN:PLAYER:Ｖ
	STAIN:PLAYER:Ｖ |= STAIN:口
ENDIF
;奴隷のＶ⇔調教者の口の汚れが移動（半人半妖、ふたなり、オトコはペニス）
IF CHECK_PENIS(TARGET)
	STAIN:Ｐ |= STAIN:PLAYER:口
	STAIN:PLAYER:口 |= STAIN:Ｐ
ELSE
	STAIN:Ｖ |= STAIN:PLAYER:口
	STAIN:PLAYER:口 |= STAIN:Ｖ
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;EXP:口淫経験 ++
;EXP:PLAYER:口淫経験 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(39 + HETEROSEX(TARGET,PLAYER)) += 8
	EXP:PLAYER:(39 + HETEROSEX(TARGET,PLAYER)) += 8
ENDIF

;愛情経験
SIF TALENT:恋慕 && ASSIPLAY == 0
	EXP:愛情経験 += 1

;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
;SIF TALENT:TARGET:猫舌
;	EXP:嗜虐快楽経験 += 1

;調教者が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
;SIF TALENT:PLAYER:猫舌
;	EXP:PLAYER:嗜虐快楽経験 += 1

;精液汚れ
SIF STAIN:PLAYER:Ｐ & 4
	EXP:精飲経験 ++

RETURN 1

@CAN_SCOM1(ARG)
SKIPDISP ARG
PRINTL シックスナイン
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 2
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF

IF MARK:快楽依存
	SIF S
		PRINT  + 
	A += MARK:1 * 2
	PRINT 快楽依存
	PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:欲情,5) * 2
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 2,')
	S = 1
ENDIF
;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 3
	PRINT 恥じらい
	PRINTV '(,3,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
		PRINT  + 
	A += 3
	PRINT 汚臭鈍感
	PRINTV '(,3,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	PRINT  - 
	A -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINT 献身的
	PRINTV '(,6,')
	S = 1
ENDIF
;快感に素直
IF TALENT:快感に素直
	SIF S
		PRINT  + 
	A += 3
	PRINT 快感に素直
	PRINTV '(,3,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && !TALENT:淫乱
	PRINT  - 
	A -= 3
	PRINT 快感の否定
	PRINTV '(,3,')
	S = 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 恋慕
	PRINTV '(,5,')
	S = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 4
	PRINT 親愛
	PRINTV '(,4,')
	S = 1
ENDIF

;媚薬
IF TEQUIP:媚薬
	SIF S
		PRINT  + 
	A += 8
	PRINT 媚薬
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:Ｖ & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:Ｖ & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:Ｖ & 8
	Y += 7
SIF STAIN:PLAYER:Ｖ & 16
	Y += 15
Y = Y * (2 - TALENT_LV(TARGET, "汚臭耐性")) / 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭敏感
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(33以上で実行)
PRINT  = 
PRINTV A

V = 33
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
RETURN 1
