LOCALS:1 = 絶頂Ｖ
LOCALS:2 = 絶頂Ａ
LOCALS:3 = 絶頂Ｂ
LOCALS:4 = 絶頂Ｍ
FOR LOCAL, 0, 5
	IF LOCAL < 4
		LOCAL:1 = NOWEX:LOCAL
	ELSE
		LOCAL:1 = NOWEX:6
	ENDIF
	SIF LOCAL:1 <= 0
		CONTINUE
	SELECTCASE LOCAL:1
		CASE 1
			
		CASE 2
			PRINT 強
		CASE 3
			PRINT 超強
		CASE 4
			PRINT 最強
		CASEELSE
			PRINT エラー
	ENDSELECT
	PRINTFORML %LOCALS:LOCAL%
NEXT
