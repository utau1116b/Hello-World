@SHOW_RATE_VO

;PRINTFORM {TFLAG:VOレート/10,4}％
ABS TFLAG:VOレート/10000
LOCAL:0 = RESULT
ABS ((TFLAG:VOレート - LOCAL:0*10000) / 100)
LOCAL:1 = RESULT
PRINTFORM ×{LOCAL:0}.
IF LOCAL:1 > 9
	PRINTFORM {LOCAL:1}
ELSEIF LOCAL:1 <= 9
	PRINTFORM 0{LOCAL:1}
ELSE
ENDIF

