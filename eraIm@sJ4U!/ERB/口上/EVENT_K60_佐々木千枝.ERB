;-------------------------------------------------
;A感覚が低い時の汎用Aセックス口上
;-------------------------------------------------
@LOW_ANAL_K60
IF PALAM:3 <= 2000
	SELECTCASE RAND:2
	CASE 1
		PRINTFORMW 「%CSTR:15%…いたいです…」
	CASE 0
		PRINTFORML 「あぁっ…いたっ！」
		PRINTFORMW 「も、もうちょっとぬるぬるしてないとだめです……」
	ENDSELECT
ELSE
	SELECTCASE RAND:2
	CASE 1
		PRINTFORML 「うぁ……大き…」
		PRINTFORMW 「…はいりましたか？　それじゃあ、やさしくしてくださいね」
	CASE 0
		PRINTFORML 「んぅ…あぁっ…！」
		PRINTFORMW 「千枝は大丈夫ですから……%CSTR:12%のしたいようにうごいてください……」
	ENDSELECT
ENDIF
