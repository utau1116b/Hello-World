FOR LOCAL:0, 1, 7
	IF CFLAG:(ARG:0):(LOCAL:0 + 1000) > LOCAL:(LOCAL:0)
		;上がっていたら上昇量表示
		SELECTCASE LOCAL:0
			CASE 1
