		;上がっていたら上昇量表示
		SELECTCASE LOCAL:0
			CASE 1
				CALL SET_COLOR(11)
				PRINTFORM POW
				RESETCOLOR
			CASE 2
				CALL SET_COLOR(20)
				PRINTFORM GUA
				RESETCOLOR
			CASE 3
				CALL SET_COLOR(24)
				PRINTFORM DEX
				RESETCOLOR
			CASE 4
				CALL SET_COLOR(21)
				PRINTFORM HIT
				RESETCOLOR
			CASE 5
				CALL SET_COLOR(12)
				PRINTFORM SPD
				RESETCOLOR
			CASE 6
				CALL SET_COLOR(13)
				PRINTFORM LUC
				RESETCOLOR
		ENDSELECT
		PRINTFORM +{(CFLAG:(ARG:0):(LOCAL:0 + 1000) - LOCAL:(LOCAL:0))}  
	ENDIF
NEXT
PRINTW 
