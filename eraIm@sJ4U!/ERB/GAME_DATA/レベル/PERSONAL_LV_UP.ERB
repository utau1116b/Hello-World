;ARG:1 現在のレベル
@IDOL_LV_UP_20(ARG:1, ARG:0)
VARSET LOCAL
VARSET LOCAL
;実際に能力アップ
FOR LOCAL:0, 1, 7
	;アップ前の値記憶
	LOCAL:(LOCAL:0) = CFLAG:(ARG:0):(LOCAL:0 + 1000)
NEXT
;能力アップ
IF ARG:1 <= 50
	BASE:(ARG:0):ベースPOW += GROW_50_POW(ARG, 78, 11)
	
	BASE:(ARG:0):ベースGUA += GROW_50_GUA(ARG, 60, 5)
	
	BASE:(ARG:0):ベースSPD += GROW_50_SPD(ARG, 72, 8)
	
	BASE:(ARG:0):ベースHIT += GROW_50_HIT(ARG, 72, 9)
	
	BASE:(ARG:0):ベースDEX += GROW_50_DEX(ARG, 72, 17)
	
