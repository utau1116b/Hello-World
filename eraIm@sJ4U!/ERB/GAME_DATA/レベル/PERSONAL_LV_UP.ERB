FOR LOCAL:0, 1, 9
	;アップ前の値記憶
	LOCAL:(LOCAL:0) = CFLAG:(ARG:0):(LOCAL:0 + 1000)
NEXT
LOCAL:10 = MAXBASE:(ARG:0):HP
LOCAL:11 = MAXBASE:(ARG:0):MP
;能力アップ

;攻撃力13
IF ARG:1 % 3 == 0
	BASE:(ARG:0):ベースPOW += 2
ELSE
	BASE:(ARG:0):ベースPOW += 1
ENDIF


;防御力7
IF ARG:1 % 10 == 0
	BASE:(ARG:0):ベースGUA += 1
ELSE
	BASE:(ARG:0):ベースGUA += 1
ENDIF


;素早さ7
IF ARG:1 % 3 == 0
	BASE:(ARG:0):ベースSPD += 2
ELSE
	BASE:(ARG:0):ベースSPD += 2
ENDIF
;命中7
IF ARG:1 % 3 == 0
	BASE:(ARG:0):ベースHIT += 1
ELSE
	BASE:(ARG:0):ベースHIT += 1
ENDIF


;運

PRINTFORML %CALLNAME:(ARG:0)%のLvが{ARG:1}になった!
FOR LOCAL:0, 1, 6
	IF CFLAG:(ARG:0):(LOCAL:0 + 1000) > LOCAL:(LOCAL:0)
		;上がっていたら上昇量表示
		SELECTCASE LOCAL:0
			CASE 1
				CALL SET_COLOR(11)
				PRINTFORM POW
				RESETCOLOR
			CASE 2
				CALL SET_COLOR(20)
				PRINTFORM GUA
				RESETCOLOR
		ENDSELECT
		PRINTFORM +{(CFLAG:(ARG:0):(LOCAL:0 + 1000) - LOCAL:(LOCAL:0))}  
	ENDIF
NEXT
IF BASE:(ARG:0):ベースSPD > LOCAL:7
	CALL SET_COLOR(12)
	PRINTFORM SPD
	RESETCOLOR
	PRINTFORM +{(BASE:(ARG:0):ベースSPD - LOCAL:7)}  
ENDIF
IF BASE:(ARG:0):ベースHIT > LOCAL:6
	PRINT HIT
	PRINTFORM +{(BASE:(ARG:0):ベースHIT - LOCAL:6)}  
ENDIF
