	BASE:(ARG:0):ベースGUA += GROW_99_GUA(ARG, 76, 60)
	
	BASE:(ARG:0):ベースSPD += GROW_99_SPD(ARG, 107, 72)
	
	BASE:(ARG:0):ベースHIT += GROW_99_HIT(ARG, 107, 72)
	
	BASE:(ARG:0):ベースDEX += GROW_99_DEX(ARG, 107, 72)
	
ENDIF

