;レベルアップ
@LV_UP
;LOCALリスト
;0	ループで使用

;初期化
REPEAT 99
	LOCAL:0 = 0
	FOR LOCAL:0, 1080, 1085
		IF FLAG:(LOCAL:0) > 0
			CALL GET_NEXT_LV_EXP(BASE:(FLAG:(LOCAL:0)):戦闘レベル, (FLAG:(LOCAL:0)))
			IF EXP:(FLAG:(LOCAL:0)):110 >= R
				EXP:(FLAG:(LOCAL:0)):110 -= R
				CFLAG:(FLAG:(LOCAL:0)):レベル保存 += R
				BASE:(FLAG:(LOCAL:0)):戦闘レベル += 1
				TRYCCALLFORM IDOL_LV_UP_{(FLAG:(LOCAL:0))}(BASE:(FLAG:(LOCAL:0)):戦闘レベル, (FLAG:(LOCAL:0)))
				CATCH
					PRINTFORMW エラー！未実装のID{(FLAG:(LOCAL:0))}が指定されました
				ENDCATCH
			ELSE
				CONTINUE
			ENDIF
		ENDIF
	NEXT
REND

;全員のレベル上昇をチェック
@CHECK_LV_UP
;
REPEAT 99
	LOCAL:0 = 0
	FOR LOCAL:0, 11, (FLAG:キャラ数 + 11)
		IF CFLAG:(LOCAL:0):勧誘状況 == -1 && LOCAL != MASTER
			CALL GIRLS_POW_EXP(LOCAL:0)
			CALL アイドルランク変化(LOCAL:0)
			CALL GET_NEXT_LV_EXP(BASE:(LOCAL:0):戦闘レベル, (LOCAL:0))
			IF EXP:(LOCAL:0):110 >= R
				EXP:(LOCAL:0):110 -= R
				CFLAG:(LOCAL:0):レベル保存 += R
				BASE:(LOCAL:0):戦闘レベル += 1
				TRYCCALLFORM IDOL_LV_UP_{(LOCAL:0)}(BASE:(LOCAL:0):戦闘レベル, LOCAL:0)
				CATCH
					PRINTFORMW エラー！未実装のID{LOCAL:0}が指定されました
				ENDCATCH
			ELSE
				CONTINUE
			ENDIF
		ELSE
			CONTINUE
		ENDIF
	NEXT
REND
