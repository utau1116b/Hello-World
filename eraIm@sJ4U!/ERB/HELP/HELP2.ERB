;アイテム解説メインページ
;--------------------------------------------------
@DICTIONARY_ITEM_MAIN
VARSET LOCAL, 0
;表示させるアイテムを抽出
REPEAT 100
	;存在しないアイテムは弾く
	SIF STRLENS(ITEMNAME:COUNT) > 0
		LOCAL:2 += 1
	;酒やお香系の判定
	SIF STRLENS(ITEMNAME:(COUNT+600)) > 0
		LOCAL:2 += 1
	;最大ページ数を示す
	SIF (LOCAL:2 % 60) == 1 && LOCAL:2 > 60
		LOCAL:1 += 1
REND
$PRINT_LIST
DRAWLINE
PRINTFORML 「アイテム」に関する大まかな説明です。 ＜page.{LOCAL+1}/{LOCAL:1+1}＞
DRAWLINE
;各表示
LOCAL:3 = 0
REPEAT 700
	;キャラカード系を除外
	SIF COUNT > 99 && COUNT < 600
		CONTINUE
	;存在しないアイテムorキャラクターカードは除外
	SIF STRLENS(ITEMNAME:COUNT) == 0
		CONTINUE
	;どこからどこまでを陳列するか
	LOCAL:3 += 1
	SIF LOCAL:3 <= (LOCAL + 1) * 60 && LOCAL:3 > LOCAL * 60
		PRINTFORMLC [{COUNT, 3}] %ITEMNAME:COUNT%
REND
PRINTL 
DRAWLINE
PRINTFORMLC \@(LOCAL <= 0) ? %" " * 18% # [1001]前のページ\@
PRINTLC [1000]戻る
PRINTFORMLC \@(LOCAL >= LOCAL:1) ? %" " * 18% # [1009]後ろのページ\@
PRINTL 
$INPUT_LOOP
INPUT
IF RESULT == 1000
	RETURN 0
ELSEIF RESULT == 1001 && LOCAL > 0
	LOCAL -= 1
ELSEIF RESULT == 1009 && LOCAL < LOCAL:1
	LOCAL += 1
ELSEIF RESULT < 0 || RESULT > 700 || (RESULT > 99 && RESULT < 600) || STRLENS(ITEMNAME:RESULT) == 0
	GOTO INPUT_LOOP
ELSE
	PRINTFORML 【%ITEMNAME:RESULT%】
	TRYCALLFORM DICTIONARY_ITEM_{RESULT}
ENDIF
GOTO PRINT_LIST

