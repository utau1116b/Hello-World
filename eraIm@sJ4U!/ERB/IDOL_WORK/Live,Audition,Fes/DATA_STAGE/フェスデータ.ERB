;仕事番号
@フェスデータ(ARG)
;通常フェス
SIF ARG != 999
	TRYCALLFORM フェスデータ_{GET_WORK_DIFFICULTY(ARG)}
;昇格試験の時
IF ARG == 999
	SELECTCASE FLAG:24
		CASE 0
			CALLFORM フェスデータ_1
			CALLNAME:1 = シャドー★ラクシュミ
		CASE 1
			CALLFORM フェスデータ_2
			CALLNAME:1 = シャドー★ドリラー
		CASE 2
			CALLFORM フェスデータ_3
			CALLNAME:1 = シャドー★アルテミス
		CASE 3
			CALLFORM フェスデータ_5
			CALLNAME:1 = シャドー★プリンス
		CASE 4
			CALLFORM フェスデータ_6
			CALLNAME:1 = シャドー★マーズ
		CASE 5
			CALLFORM フェスデータ_8
			CALLNAME:1 = 閣下
		CASE 6
			CALLFORM フェスデータ_9
			CALLNAME:1 = シャドー★ルナ
		CASE 7
			CALLFORM フェスデータ_10
			CALLNAME:1 = シャドー★ヴィーナス
		CASE 8
			CALLFORM フェスデータ_11
			CALLNAME:1 = シャドー★セイレーン
		CASE 9
			CALLFORM フェスデータ_11
			CALLNAME:1 = シャドー★プリンセス
		CASE 10
			CALLFORM フェスデータ_11
			CALLNAME:1 = シャドー★アリス
		CASEELSE
			CALLFORM フェスデータ_1
			CALLNAME:1 = エラー番号{FLAG:24}
	ENDSELECT
ENDIF
