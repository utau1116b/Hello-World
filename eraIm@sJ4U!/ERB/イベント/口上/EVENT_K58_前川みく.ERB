;〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜
;コミュニケーション系コマンド
;〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜
;300番台は TFLAG:18 = 1 大成功 TFLAG:18 = 0 成功 TFLAG:18 = -1 失敗
;-------------------------------------------------
;レッスン CFLAG401
;-------------------------------------------------
;-------------------------------------------------
;TFLAG:17  0 = 自主トレ 1 = Voトレ 2 = Viトレ 3 = Daトレ 
;		   4 = 超Voトレ 5 = 超Viトレ 6 = 超Daトレ 7 = 地獄の特訓 
;-------------------------------------------------
IF SELECTCOM == 300
	;初めて
	IF CFLAG:401 == 0
		PRINTFORMW 「みくがんばるにゃあ！」
		CFLAG:401 = 1
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:85 == 1 && (CFLAG:401 < 3 || FLAG:7 == 2)
			;失敗
			IF TFLAG:18 == -1
				PRINTFORMW 「うにゃぁ…調子が悪いにゃん……」
			;Vo系トレ
			ELSEIF (TFLAG:17 == 1 || TFLAG:17 == 4)
				IF RAND:2
					PRINTFORMW 「%CALLNAME:MASTER%チャンを誘惑するつもりで声出してみたにゃ…どぉ？」
				ELSE
					PRINTFORMW 「にゃー♪　テステス♪　ンー、今日のみくは絶好調にゃ！」
				ENDIF
			;Vi系トレ
			ELSEIF (TFLAG:17 == 2 || TFLAG:17 == 5)
				IF RAND:2
					PRINTFORMW 「もっとセクシーでキュートなみくを見せるにゃ☆」
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%チャン！　見て見て〜」
				ENDIF
			;Da系トレ
			ELSEIF (TFLAG:17 == 3 || TFLAG:17 == 6)
				IF RAND:2
					PRINTFORMW 「みくに見とれてもいいにゃあ♪」
				ELSE
					PRINTFORMW 「みくのダンスで%CALLNAME:MASTER%チャンには効果抜群？」
				ENDIF
			;地獄
			ELSEIF TFLAG:17 == 7
				PRINTFORMW 「はぁ…はぁっ…この特訓はキッツイにゃあ…」
			;その他
			ELSEIF RAND:2
				PRINTFORMW 「%CALLNAME:MASTER%チャン%CALLNAME:MASTER%チャン！」
				PRINTFORMW 「みくどうだった？　上手く出来たかにゃ？」
			ELSE
				PRINTFORMW 「よし、これでいいかな%CALLNAME:MASTER%チャン」
			ENDIF
			CFLAG:401 = 3
		;それ以外
		ELSEIF CFLAG:401 < 2 || FLAG:7 == 2
			;失敗
			IF TFLAG:18 == -1
				PRINTFORMW 「あれれ？　間違っちゃったかにゃ？」
			;Vo系トレ
			ELSEIF (TFLAG:17 == 1 || TFLAG:17 == 4)
				PRINTFORMW 「にゃ〜♪　にゃー♪」
			;Vi系トレ
			ELSEIF (TFLAG:17 == 2 || TFLAG:17 == 5)
				PRINTFORMW 「%CALLNAME:MASTER%チャン、みくかわいいっしょ〜☆」
			;Da系トレ
			ELSEIF (TFLAG:17 == 3 || TFLAG:17 == 6)
				PRINTFORMW 「にゃんにゃんダンスってどう？」
			;地獄
			ELSEIF TFLAG:17 == 7
				PRINTFORMW 「む、無理だにゃ！　死んでしまうにゃ〜！！」
			;その他
			ELSE
				IF RAND:2
					PRINTFORMW 「おーしまいっ！！」
				ELSE
					PRINTFORMW 「うん、まずまずの出来だにゃ！」
				ENDIF
			ENDIF
			CFLAG:401 = 2
		ENDIF
	ENDIF
ENDIF

