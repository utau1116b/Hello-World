;状態変化チェック
;ARG:0	対象キャラ登録番号
;どう変化させるかなどはTFLAGを見る
@CHECK_CHANGE_STATE(ARG:0)
;LOCAL
;0	LOOP
;1	LOOP
;2	発動率一時保存
;3	ID一時保存
;4	抵抗確率一時保存
;5	残り時間一時保存
;6	何らかの効果を発揮したフラグ（戻り値）

FOR LOCAL:0, 1, 7
	LOCAL:(LOCAL:0) = 0
NEXT

;スリップ〜気絶
FOR LOCAL:0, 1041, 1050
	IF TFLAG:(LOCAL:0)
		LOCAL:2 = TFLAG:(LOCAL:0) % 1000
		LOCAL:4 = 100 - CFLAG:(ARG:0):(LOCAL:0 - 21)
		LOCAL:5 = TFLAG:(LOCAL:0) / 1000
		
		LOCAL:2 = LOCAL:2 * LOCAL:4 / 100
		
		IF RAND:100 < LOCAL:2
			IF LOCAL:0 < 1044 || LOCAL:0 == 1049
				;時間経過で治らないものはフラグを立てるだけ
				IF CFLAG:(ARG:0):(LOCAL:0 - 10) == 0
					;新しくこの状態になる場合、メッセージを表示
					CALL SHOW_SET_STATE_MESSAGE(ARG:0, (LOCAL:0 - 1040))
					LOCAL:6 = 1
				ENDIF
				;
				CFLAG:(ARG:0):(LOCAL:0 - 10) = 1
			ELSE
				;他は普通に残り時間を代入
				IF CFLAG:(ARG:0):(LOCAL:0 - 10) < 10 * LOCAL:5
					;効果時間が延長される場合、メッセージを表示＋効果反映
					CALL SHOW_SET_STATE_MESSAGE(ARG:0, (LOCAL:0 - 1040))
					CFLAG:(ARG:0):(LOCAL:0 - 10) = 10 * LOCAL:5
					LOCAL:6 = 1
				ENDIF
			ENDIF
		ENDIF
	ENDIF
NEXT



;スリップ〜気絶を治療
FOR LOCAL:0, 1051, 1060
	IF TFLAG:(LOCAL:0) && CFLAG:(ARG:0):(LOCAL:0 - 20)
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, (LOCAL:0 - 950))
		CALL BMSG_WAIT(2)
		CFLAG:(ARG:0):(LOCAL:0 - 20) = 0
		IF LOCAL:0 == 1059
			CFLAG:(ARG:0):現在状態 = 0
			IF BASE:(ARG:0):HP <= 0
				BASE:(ARG:0):HP = 1
			ENDIF
		ENDIF
		LOCAL:6 = 1
	ENDIF
NEXT

;ステ変化
;2	発動率一時保存
;3	ID一時保存
;4	抵抗確率一時保存
;5	残り時間一時保存
;6	何らかの効果を発揮したフラグ（戻り値）
FOR LOCAL:0, 1060, 1068
	IF TFLAG:(LOCAL:0) > 0
		LOCAL:2 = (TFLAG:(LOCAL:0) % 1000)
		LOCAL:4 = 100 - CFLAG:(ARG:0):能力低下抵抗率
		LOCAL:5 = TFLAG:(LOCAL:0) / 1000
		
		;個別耐性反映
		LOCAL:4 = LOCAL:4 * (100 - CFLAG:(ARG:0):(LOCAL:0 + 250)) / 100
		
		;能力低下でなければ抵抗されない
		IF (LOCAL:5 % 1000) >= 100
			LOCAL:4 = 100
		ENDIF
		
		LOCAL:2 = LOCAL:2 * LOCAL:4 / 100
		IF RAND:100 < LOCAL:2
			;残り時間や変化量に関わらず、強制上書き
			CALL SHOW_SET_STATE_MESSAGE(ARG:0, (LOCAL:0  - 900))
			;値を代入
			CFLAG:(ARG:0):(LOCAL:0 + 70) = LOCAL:5
			LOCAL:6 = 1
		ELSEIF (TFLAG:(LOCAL:0) % 1000) >= 100
			;100%成功の物が失敗になった場合は誤魔化せないのでメッセージを出す
			PRINTFORML %CALLNAME:(ARG:0)%は能力低下に抵抗した！
			CALL BMSG_WAIT(1)
		ENDIF
	ELSEIF TFLAG:(LOCAL:0) < 0 && CFLAG:(ARG:0):(LOCAL:0 + 70)
		;リセットの場合
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, (LOCAL:0 - 900))
		CFLAG:(ARG:0):(LOCAL:0 + 70) = 0
		LOCAL:6 = 1
	ENDIF
NEXT


;未完成
;加算系ステ変化
FOR LOCAL:0, 1126, 1130
;LOCAL:2	確率
;LOCAL:5	例30130→時間、上昇量
;LOCAL:4	能力低下耐性
;LOCAL:5が130なら実際は130−100で30アップ
	IF TFLAG:(LOCAL:0) > 0
		LOCAL:2 = (TFLAG:(LOCAL:0) % 1000)
		LOCAL:4 = 100 - CFLAG:(ARG:0):能力低下抵抗率
		LOCAL:5 = TFLAG:(LOCAL:0) / 1000
		
		;加算系の耐性はまだ設定していないので、今のところ確実に効く
		;個別耐性反映
		;LOCAL:4 = LOCAL:4 * (100 - CFLAG:(ARG:0):(LOCAL:0 - 200 + 500)) / 100
		
		;能力低下でなければ抵抗されない
		IF (LOCAL:5 % 1000) >= 100
			LOCAL:4 = 100
		ENDIF
		LOCAL:2 = LOCAL:2 * LOCAL:4 / 100
		IF RAND:100 < LOCAL:2
			;値を代入
			;CFLAG:(ARG:0):(LOCAL:0 + 100 - 200) = LOCAL:5
			;POW
			IF TFLAG:加算POW
				CFLAG:(ARG:0):変化加算POW = LOCAL:5
				CALL SHOW_SET_STATE_MESSAGE(ARG:0, 162)
			ENDIF
			;GUA
			IF TFLAG:加算GUA
				CFLAG:(ARG:0):変化加算GUA = LOCAL:5
				CALL SHOW_SET_STATE_MESSAGE(ARG:0, 163)
			ENDIF
			;SPD
			IF TFLAG:加算SPD
				CFLAG:(ARG:0):変化加算SPD = LOCAL:5
				CALL SHOW_SET_STATE_MESSAGE(ARG:0, 166)
			ENDIF
			;HIT
			;IF TFLAG:加算HIT
			;	CFLAG:(ARG:0):変化加算HIT = LOCAL:5
			;	CALL SHOW_SET_STATE_MESSAGE(ARG:0, 165)
			;ENDIF
			LOCAL:6 = 1
		ELSEIF (TFLAG:(LOCAL:0) % 1000) >= 100
			;100%成功の物が失敗になった場合は誤魔化せないのでメッセージを出す
			PRINTFORML %CALLNAME:(ARG:0)%は能力低下に抵抗した！
		ENDIF
	ELSEIF TFLAG:加算POW < 0 && CFLAG:(ARG:0):変化加算POW
		;リセットの場合
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, 162)
		CFLAG:(ARG:0):変化加算POW = 0
		LOCAL:6 = 1
	ELSEIF TFLAG:加算GUA < 0 && CFLAG:(ARG:0):変化加算GUA
		;リセットの場合
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, 163)
		CFLAG:(ARG:0):変化加算GUA = 0
		LOCAL:6 = 1
	ELSEIF TFLAG:加算SPD < 0 && CFLAG:(ARG:0):変化加算SPD
		;リセットの場合
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, 166)
		CFLAG:(ARG:0):変化加算SPD = 0
		LOCAL:6 = 1
	;ELSEIF TFLAG:加算HIT < 0 && CFLAG:(ARG:0):変化加算HIT
		;リセットの場合
	;	CALL SHOW_CURE_STATE_MESSAGE(ARG:0, 165)
	;	CFLAG:(ARG:0):変化加算HIT = 0
	;	LOCAL:6 = 1
	ENDIF
NEXT



;耐性変化
FOR LOCAL:0, 1068, 1081
	IF TFLAG:(LOCAL:0) > 0
		LOCAL:2 = (TFLAG:(LOCAL:0) % 1000)
		LOCAL:5 = TFLAG:(LOCAL:0) / 1000
		
		;とりあえず耐性変化は必ず効くようにする
		IF RAND:100 < LOCAL:2
			;残り時間や変化量に関わらず、強制上書き
			CALL SHOW_SET_STATE_MESSAGE(ARG:0, (LOCAL:0 - 900))
			;値を代入
			CFLAG:(ARG:0):(LOCAL:0 + 70) = LOCAL:5
			LOCAL:6 = 1
		ELSEIF (TFLAG:(LOCAL:0) % 1000) >= 100
			PRINTFORML %CALLNAME:(ARG:0)%は耐性変化に抵抗した！
			CALL BMSG_WAIT(1)
		ENDIF
	ELSEIF TFLAG:(LOCAL:0) < 0 && CFLAG:(ARG:0):(LOCAL:0 + 70)
		;リセットの場合
		CALL SHOW_CURE_STATE_MESSAGE(ARG:0, (LOCAL:0 - 900))
		CFLAG:(ARG:0):(LOCAL:0 + 70) = 0
		LOCAL:6 = 1
	ENDIF
NEXT

;能力低下治療の場合
IF TFLAG:能力低下治療
	FOR LOCAL:0, 1130, 1151
		IF (CFLAG:(ARG:0):(LOCAL:0) % 1000) < 100 && CFLAG:(ARG:0):(LOCAL:0) != 0
			SETCOLOR 0x10EEEE
			CALL SHOW_CURE_STATE_MESSAGE(ARG:0, (LOCAL:0 - 1000 + 32))
			CFLAG:(ARG:0):(LOCAL:0) = 0
			LOCAL:6 = 1
		ENDIF
	NEXT
ENDIF



;強制状態異常変化
IF TFLAG:特殊状態変化
	LOCAL:2 = TFLAG:特殊状態変化 % 1000
	LOCAL:3 = TFLAG:特殊状態変化 / 1000
	LOCAL:5 = TFLAG:特殊状態変化値
	IF RAND:100 < LOCAL:2
		IF CFLAG:(ARG:0):(LOCAL:3) < LOCAL:5
			;残り時間や効果量が上昇する場合のみ更新
			CALL SHOW_SET_STATE_MESSAGE(ARG:0, LOCAL:3 - 1000)
			CFLAG:(ARG:0):(LOCAL:3) = LOCAL:5
			LOCAL:6 = 1
		ELSEIF LOCAL:5 < 0
			;マイナスの効果時間は強制治療
			CALL SHOW_CURE_STATE_MESSAGE(ARG:0, LOCAL:3 - 1000)
			CFLAG:(ARG:0):(LOCAL:3) = 0
			LOCAL:6 = 1
		ENDIF
	ENDIF
	;未実装
	;IF LOCAL:3 == 1120
	;	;変身状態になる＝変身完了なので、服などを元に戻す
	;	CALL HENSHIN_END(ARG:0)
	;ENDIF
ENDIF
	
RETURN LOCAL:6

